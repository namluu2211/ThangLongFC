{
  "name": "thanglong-fc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "ng serve --open",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "build:prod": "ng build --configuration production --base-href=https://namluu2211.github.io/ThangLongFC/",
    "deploy": "npm run build:prod && npx angular-cli-ghpages --dir=dist/browser --branch=gh-pages --no-silent",
    "deploy:skip-hooks": "npm run build:prod && git config core.hooksPath nul && npx angular-cli-ghpages --dir=dist/browser --branch=gh-pages --no-silent && git config --unset core.hooksPath",
    "deploy:force": "npm run build:prod && set SKIP_GITLEAKS=1 && set SKIP_SENSITIVE_CHECK=1 && npx angular-cli-ghpages --dir=dist/browser --branch=gh-pages --no-silent"
  },
  "dependencies": {
    "@angular/animations": "^20.3.1",
    "@angular/cdk": "^20.2.4",
    "@angular/common": "^20.3.1",
    "@angular/compiler": "^20.3.1",
    "@angular/core": "^20.3.1",
    "@angular/forms": "^20.3.1",
    "@angular/platform-browser": "^20.3.1",
    "@angular/platform-browser-dynamic": "^20.3.1",
    "@angular/router": "^20.3.1",
    "chart.js": "^4.4.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.2",
    "@angular/cli": "^20.3.2",
    "@angular/compiler-cli": "^20.3.1",
    "angular-cli-ghpages": "^2.0.3",
    "typescript": ">=5.5.0 <5.9.0"
  }
}